<h1> <%= @doctor.name %> </h1>

<% @doctor.appointments.each do |appointment| %>
  <p><%= appointment.appointment_datetime.strftime('%B %d, %Y') %> at <%=appointment.appointment_datetime.strftime('%k:%M')  %></p>
  <p> <%= link_to appointment.patient.name, patient_path(appointment.patient) %>
<% end %>
